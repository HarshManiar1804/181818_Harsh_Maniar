generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Store {
  id    String @id
  label String
  city  String
  state String
}

model SKUs {
  id         String @id
  label      String
  class      String
  department String
  price      Float
  cost       Float
}

model Calendar {
  id         Int    @id 
  week       String
  weekLabel  String
  month      String
  monthLabel String
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model Planning {
  id         Int    @id 
  store      String
  sku        String
  week       String
  salesUnits Int

  @@ignore
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model Calculation {
  id           Int    @id
  store        String
  sku          String
  week         String
  salesUnits   String
  salesDollars String
  costDollars  String
  gmDollars    String
  gmPercent    String

  @@ignore
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model Chart {
  id           Int    @id 
  week         String
  gmDollars    String
  salesDollars String
  gmPercent    String

  @@ignore
}
